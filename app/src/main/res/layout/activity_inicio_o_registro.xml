<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="inicioORegistroVM" type="com.chatbotthalia.app.modules.iniciooregistro.data.viewmodel.InicioORegistroVM"/>
    </data>
    <FrameLayout android:id="@+id/frameIniciooregist"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumntimelight"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:gravity="center_horizontal"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_width="match_parent"
                app:contentInsetStart="0dp" style="@style/groupStyleblack_900">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearBase"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginEnd="@dimen/_13pxh"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageTimeLight"
                        android:layout_height="@dimen/_11pxh"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_17pxv"
                        android:layout_width="@dimen/_28pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_timelight}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_timelight"/>
                    <FrameLayout android:id="@+id/frameStacknotch"
                        android:layout_height="@dimen/_28pxv"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginStart="@dimen/_8pxh" android:layout_width="@dimen/_275pxh">
                        <ImageView android:id="@+id/imageNotch"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_28pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="@dimen/_219pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_notch"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_notch"/>
                        <ImageView android:id="@+id/imageRightSide"
                            android:layout_gravity="bottom|end"
                            android:layout_height="@dimen/_11pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="@dimen/_66pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_rightside"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rightside"/>
                    </FrameLayout>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtThalia"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_76pxh"
                android:layout_marginStart="@dimen/_77pxh"
                android:layout_marginTop="@dimen/_39pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{inicioORegistroVM.inicioORegistroModel.txtThalia}"
                style="@style/txtArchivoBlackregular60" tools:text="@string/lbl_thalia"/>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtTeayudareaen"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_75pxh"
                android:layout_marginStart="@dimen/_77pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:singleLine="true"
                android:text="@{inicioORegistroVM.inicioORegistroModel.txtTeayudareaen}"
                style="@style/txtSyneregular20" tools:text="@string/msg_te_ayudare_a_en"/>
            <FrameLayout android:id="@+id/frameStackrectangle"
                android:layout_gravity="end"
                android:layout_height="@dimen/_343pxv"
                android:layout_marginEnd="@dimen/_113pxh"
                android:layout_marginStart="@dimen/_113pxh"
                android:layout_marginTop="@dimen/_96pxv" android:layout_width="@dimen/_111pxh">
                <ImageView android:id="@+id/imageRectangle"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_191pxv"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_rectangle}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle"/>
                <ImageView android:id="@+id/imageRectangleOne"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_143pxv"
                    android:layout_marginBottom="@dimen/_91pxv"
                    android:layout_marginTop="@dimen/_91pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_rectangle_143x111}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_143x111"/>
                <FrameLayout android:id="@+id/frameStackrectangleTwo"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_143pxv"
                    android:layout_marginBottom="@dimen/_10pxv" android:layout_width="@dimen/_111pxh">
                    <ImageView android:id="@+id/imageRectangleTwo"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_143pxh"
                        android:layout_width="@dimen/_111pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_rectangle_1}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_1"/>
                    <ImageView android:id="@+id/imageRectangleThree"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_143pxh"
                        android:layout_width="@dimen/_111pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_rectangle_1}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_1"/>
                </FrameLayout>
            </FrameLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnRegistrate"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_36pxv"
                android:layout_marginEnd="@dimen/_77pxh"
                android:layout_marginStart="@dimen/_77pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="@dimen/_160pxh"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:paddingBottom="@dimen/_7pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_7pxv"
                android:singleLine="true"
                android:text="@string/lbl_registrate"
                android:textAllCaps="false"
                style="@style/btnSolidRounded" tools:text="@string/lbl_registrate"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowlinetwo"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_77pxh"
                android:layout_marginStart="@dimen/_77pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <View android:background="@color/orange_A700"
                    android:id="@+id/lineLineTwo"
                    android:layout_height="@dimen/_3pxv"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_weight="0.46" android:layout_width="0dp"/>
                <TextView android:ellipsize="end" android:id="@+id/txtO"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_weight="0.07"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{inicioORegistroVM.inicioORegistroModel.txtO}"
                    style="@style/txtSyneregular17" tools:text="@string/lbl_o"/>
                <View android:background="@color/orange_A700"
                    android:id="@+id/lineLineOne"
                    android:layout_height="@dimen/_3pxv"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_weight="0.47" android:layout_width="0dp"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtIniciaSesin"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_77pxh"
                android:layout_marginStart="@dimen/_77pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:singleLine="true"
                android:text="@{inicioORegistroVM.inicioORegistroModel.txtIniciaSesin}"
                style="@style/txtSyneregular17" tools:text="@string/lbl_inicia_sesi_n"/>
            <LinearLayout android:id="@+id/linearRowpolygonone"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_34pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStyleblack_901">
                <ImageView android:id="@+id/imagePolygonOne"
                    android:layout_height="@dimen/_35pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="@dimen/_48pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_32pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_polygon1"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_polygon1"/>
                <LinearLayout android:id="@+id/linearRowellipseone"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginEnd="@dimen/_62pxh"
                    android:layout_marginStart="@dimen/_86pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_weight="0.8"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <View
                        android:background="@drawable/rectangle_bg_white_a700_radius_15_5"
                        android:id="@+id/viewEllipseOne"
                        android:layout_height="@dimen/_31pxh" android:layout_width="@dimen/_31pxh"/>
                    <View android:background="@color/white_A700"
                        android:id="@+id/lineLineThree"
                        android:layout_height="@dimen/_26pxv"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginStart="@dimen/_76pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_weight="0.07" android:layout_width="0dp"/>
                    <View android:background="@color/white_A700"
                        android:id="@+id/lineLineFive"
                        android:layout_height="@dimen/_26pxv"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_weight="0.07" android:layout_width="0dp"/>
                    <View android:background="@color/white_A700"
                        android:id="@+id/lineLineFour"
                        android:layout_height="@dimen/_26pxv"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_weight="0.07" android:layout_width="0dp"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
